<!DOCTYPE html>
<html lang="pt">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

	<title>{{#if title}}{{title}} - {{/if}}{{author.name}}</title>
	<meta name="description" content="{{author.tagline}}" />

	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<!-- Metatags gerais -->
	<meta name="keywords" content="Networking, Startup, startup angola, Angola, empreendedores angola, angola startups">
	<meta name="copyright" content="Bantu Makers">
	<meta name="author" content="Bantu Makers, Doriel3572">
	<!-- Twitter Card -->
	<meta name="twitter:card" content="/c.png" />
	<meta name="twitter:site" content="www.Conversastartup.com" />
	<meta name="twitter:title" content="Conversas Startup" />
	<meta name="twitter:description" content="Networking descontraído para empreendedores." />
	<meta name="twitter:image" content="/c.png" />
	<meta name="twitter:url" content="http://conversastartup.com" />
	<!-- Facebook Open Graph -->
	<meta property="og:title" content="" />
	<meta property="og:site_name" content="Conversas Startup" />
	<meta property="og:url" content="http://conversastartup.com" />
	<meta property="og:description" content="Networking descontraído para empreendedores." />
	<meta property="og:image" content="/c.png" />
	    <!-- tags opcionais -->
	<meta property="og:type" content="website" />
	<meta property="og:locale" content="pt" />

	<link rel="icon" type="image/png" href="/c.png" />
	<link rel="alternate" type="application/atom+xml" href="/feed.xml" title="{{author.name}}" />

	<link rel="stylesheet" type="text/css" href="/css/main.css" />
	<link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" />

	<link rel="canonical" href="{{url}}{{postslash path}}" />
</head>
<body>
	<div id="wrap">
		{{{contents}}}
	</div>

	<footer class="site">
		<section>
			Built with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://www.bantumakers.com">Bantu Makers</a>.
		</section>
	</footer>

	<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
	{{#if jQueryNoConflict}}
	<script type="text/javascript">
		var jQuery = $.noConflict(true);
	</script>
	{{/if}}
	<script src="//netdna.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

	{{#if analytics}}
	<script>
		(function (i, s, o, g, r, a, m) {
			i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
				(i[r].q = i[r].q || []).push(arguments)
			}, i[r].l = 1 * new Date(); a = s.createElement(o),
			m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
		})(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
		ga('create', '{{analytics.id}}', '{{analytics.domain}}');
		ga('send', 'pageview');
	</script>
	{{/if}}
</body>
</html>
